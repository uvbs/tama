      </text>
      <text posx="center" posy="155" font="font_text_b" text_col="white" font_size="30" font_style="stroke" text="80145">
        <!--비밀번호-->
      </text>
      <butt posx="right" posy="59" img_up="common_butt_x.png" key="butt.close">
