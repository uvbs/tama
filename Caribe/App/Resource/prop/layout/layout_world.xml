<?xml version="1.0" encoding="utf-8"?>
<!--월드 씬-->
<layout font_text_m="normal.ttf" font_text_b="bold.ttf" font_res_num="res_num.ttf">
  <!--월드 씬 메인-->
  <world>
    <!--유저 닉네임, 경험치, 트로피-->
    <wnd posx="0" posy="16" width="0" height="0" key="wnd.user.info">
      <!--레벨/이름/경험치-->
      <img posx="16" posy="2" file="world_top_exp_bg.png" key="img.name.bg">
        <pbar posx="0" posy="1" key="pbar.exp" width="100" height="10" img1="world_top_exp.png"></pbar>
        <img posx="1" posy="1" file="world_top_exp_cover.png" />
        <img posx="-12" posy="-2" file="world_top_shield.png">
          <text posx="center" posy="center" key="text.level" font="font_res_num" font_size="20" font_style="stroke" id="666" />
        </img>
        <text posx="center" posy="center" key="text.name" font="font_text_b" font_size="20" font_style="stroke" />
        <text posx="-10" posy="14" key="text.exp" font="font_text_s" font_size="15" font_style="stroke" text="80000" show="off"></text>
      </img>
      <layout posx="0" posy="5" file="profile_picture.xml" layout="profile_me" show="off" scale="1.0" key="wnd.profile" />
      <!--승점-->
      <img posx="16" posy="25" file="world_top_blank.png" key="img.score.bg">
        <img posx="-12" posy="-2" file="world_top_trophy.png"></img>
        <text posx="center" posy="center" key="text.trophy" font="font_res_num" font_size="20" font_style="stroke" text="80000" />
      </img>
      <!--전투력-->
      <img posx="16" posy="46" file="world_top_blank.png" key="img.power.bg">
        <img posx="-12" posy="-2" file="world_top_power.png"></img>
        <text posx="center" posy="center" key="text.power" font_size="20" font_style="stroke" font="font_res_num" />
      </img>
      <!--행동력-->
      <img posx="center" posy="2" file="world_top_blank2.png">
        <img posx="-15" posy="-2" file="icon_ap.png" />
        <text posx="center" posy="center" font_size="20" font_style="stroke" key="text.ap" align="center" counter="yes" comma="yes" font="font_res_num"></text>
      </img>
      <!--캐쉬-->
      <_img posx="329" posy="2" file="world_top_blank2.png">
        <spr posx="72" posy="-1" file="ui_cash.spr" />
        <text posx="center" posy="center" font_size="20" font_style="stroke" key="text.cnt.cash" counter="yes" comma="yes" font="font_res_num"></text>
      </_img>
    </wnd>
    <!--왼쪽 버튼들-->
    <wnd posx="0" posy="0" width="0" height="0" key="wnd.left.butts">
      <!--왼쪽 버튼들 bg-->
      <img posx="0" posy="70" file="world_left_bg.png" key="img.world.left.bg" />
      <!--랭킹, 소셜, 우편함, 옵션 버튼-->
      <butt posx="3" posy="77" img_up="world_left_rank_butt.png" key="butt.world.rank">
        <!--랭킹 버튼-->
      </butt>
      <butt posx="3" posy="130" img_up="world_left_social_butt.png" key="butt.world.social">
        <!--소셜 버튼-->
      </butt>
      <butt posx="3" posy="184" img_up="world_left_mailbox_butt.png" key="butt.world.mailbox">
        <!--우편함 버튼-->
      </butt>
      <spr posx="45" posy="195" spr="ui_alert.spr" key="img.world.left.mailboxalert2" show="0" touchable="off" />
      <butt posx="3" posy="238" img_up="world_left_option_butt.png" key="butt.world.option">
        <!--옵션 버튼-->
      </butt>
      <!--숨기기 버튼-->
      <butt posx="68" posy="150" img_up="world_left_butt.png" key="butt.world.hide">
        <!--숨기기 버튼-->
      </butt>
      <!--post 알림-->
      <spr posx="95" posy="155" spr="ui_alert.spr" key="img.world.left.mailboxalert1" show="0" touchable="off" />
    </wnd>
    <!--아래 버튼들-->
    <wnd posx="0" posy="0" width="0" height="0" key="wnd.butt.main">
      <!--부대편성, 기술연구, 영웅고용, 창고, 시장 버튼-->
      <butt posx="29" posy="320" img_up="common_butt_main.png" key="butt.unitorg">
        <img posx="-12" posy="-7" file="world_bottom_region.png" />
        <text posx="center" posy="center" font="font_text_b" font_size="30" font_style="stroke" text="80004" key="text.butt.legion">
          <!--부대편성-->
        </text>
      </butt>
      <butt posx="155" posy="320" img_up="common_butt_main.png" key="butt.tech">
        <img posx="-12" posy="3" file="world_bottom_skillstudy.png" />
        <text posx="center" posy="center" font="font_text_b" font_size="30" font_style="stroke" text="80049" key="text.butt.tech" debug="32">
          <!--기술연구-->
        </text>
      </butt>
      <butt posx="279" posy="320" img_up="common_butt_main.png" key="butt.hero">
        <img posx="-12" posy="-4" file="world_bottom_hero.png" />
        <text posx="center" posy="center" font="font_text_b" font_size="30" font_style="stroke" text="80053" key="text.butt.hero">
          <!--영웅고용-->
        </text>
      </butt>
      <butt posx="405" posy="320" img_up="common_butt_main.png" key="butt.storage">
        <img posx="-15" posy="0" file="world_bottom_storage.png" />
        <text posx="center" posy="center" font="font_text_b" font_size="30" font_style="stroke" text="80051" key="text.butt.storage">
          <!--창고-->
        </text>
      </butt>
      <butt posx="529" posy="320" img_up="common_butt_main.png" key="butt.market">
        <img posx="-12" posy="2" file="world_bottom_market.png" />
        <text posx="center" posy="center" font="font_text_b" font_size="30" font_style="stroke" text="80052" key="text.butt.market">
          <!--시장-->
        </text>
      </butt>
      <!--부대편성, 기술연구, 영웅고용, 창고, 시장 이미지-->
      <!--부대편성, 기술연구, 영웅고용, 창고, 시장 텍스트-->
    </wnd>
    <!--스페셜 스팟 출현 남은기간-->
    <sound bgm="lobby" />
    <butt posx="4" posy="77" img_up="log_butt.png" key="butt.log.battle">
      <!--공격/방어 로그-->
    </butt>
  </world>
  <!--우편함 팝업-->
  <!--우편함 내용 팝업-->
  <_mailcontentpopup>
    <img posx="62" posy="9" file="common_bg_popup.png" key="img.world.mailcontent" format="argb8888">
      <!--타이틀-->
      <text posx="3" posy="17" font="font_text_b" text_col="white" font_size="43" font_style="stroke" align="center" text="80072" />
      <!--메일 제목 bg & 내용 bg-->
      <img posx="100" posy="80" file="mail_bg_title2.png" />
      <img posx="100" posy="130" file="mail_bg_content.png" />
      <!--메일 제목 & 내용 텍스트-->
      <text posx="50" posy="90" font="font_text_b" text_col="white" font_size="43" font_style="stroke" align="left" text="80077" />
      <text posx="50" posy="170" font="font_text_b" text_col="white" font_size="43" font_style="stroke" align="left" text="80078" />
      <!--메일 제목 & 내용-->
      <text posx="120" posy="90" font="font_text_b" text_col="white" font_size="30" font_style="stroke" align="left" text="80077" key="text.mail.title" />
      <text posx="110" posy="140" width="330" font="font_text_b" text_col="white" font_size="30" font_style="stroke" align="left" text="80078" key="text.mail.content" />
      <!--확득 버튼-->
      <butt posx="300" posy="282" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.get" font_style="stroke" text="80076"></butt>
      <!--확인 버튼-->
      <butt posx="404" posy="282" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.ok" font_style="stroke" text="2"></butt>
    </img>
  </_mailcontentpopup>
  <!--옵션 팝업-->
  <_optionpopup>
    <img posx="62" posy="9" file="common_bg_popup.png" key="img.world.option" format="argb8888">
      <!--타이틀-->
      <img posx="center" posy="18" img="popup_title_bar.png">
        <text posx="center" posy="4" font="font_text_b" text_col="white" font_size="43" font_style="stroke" text="80074" />
      </img>
      <!--"소리끄기", "언어선택", "진동끄기", "초대거절" 텍스트-->
      <text posx="30" posy="120" font="font_text_b" text_col="white" font_size="43" font_style="stroke" align="right" text="80079" width="100" />
      <text posx="210" posy="120" font="font_text_b" text_col="white" font_size="43" font_style="stroke" align="right" text="80080" width="100" />
      <text posx="30" posy="210" font="font_text_b" text_col="white" font_size="43" font_style="stroke" align="right" text="80081" width="100" />
      <!--체크박스 bg 1 ~ 4-->
      <img posx="140" posy="110" file="option_bg_checkbox.png" key="img.world.checkbox1">
        <!--체크 표시 1-->
        <img posx="9" posy="8" file="option_check.png" key="img.world.check1" />
      </img>
      <img posx="320" posy="110" file="option_bg_checkbox.png" key="img.world.checkbox2">
        <!--국기-->
        <img posx="9" posy="8" _file="option_ko.png" key="img.flag" />
        <list posx="9" posy="40" width="47" height="100" type="list" key="list.flag" />
      </img>
      <img posx="140" posy="200" file="option_bg_checkbox.png" key="img.world.checkbox3">
        <!--체크 표시 2-->
        <img posx="9" posy="8" file="option_check.png" key="img.world.check2" />
      </img>
      <!--닫기 버튼-->
      <butt posx="470" posy="31" img_up="common_butt_x.png" key="butt.cancel"></butt>
      <_text posx="center" posy="70" font="font_text_s" text_col="white" font_size="20" font_style="stroke" key="text.play.time" text="1" />
      <butt posx="291" posy="208" img_up="facebook_norm.png" key="butt.facebook" show="off"></butt>
    </img>
  </_optionpopup>
  <_spotclickselect>
    <img posx="222" posy="87" file="world_center_bg.png" key="img.world.center">
      <!--공격-->
      <butt posx="-10" posy="50" img_up="world_butt_attack.png" key="butt.world.attack"></butt>
      <img posx="-10" posy="50" file="world_butt_attack_d.png" key="img.world.attack" show="0" />
      <!--정찰-->
      <butt posx="65" posy="0" img_up="world_butt_recon.png" key="butt.world.recon"></butt>
      <!--수집-->
      <butt posx="70" posy="80" img_up="world_butt_collect.png" key="butt.world.collect"></butt>
      <img posx="70" posy="80" file="world_butt_collect_d.png" key="img.world.collect" show="0" />
    </img>
  </_spotclickselect>
  <quest_list posx="580" posy="16">
    <img posx="0" posy="0" file="world_right_quest_bg.png" alpha="1" format_surface="argb8888">
      <!--퀘스트 bg-->
    </img>
    <list posx="18" posy="20" width="34" height="187" type="list" key="list.quest" debug="1" />
    <_butt posx="18" posy="20" img_up="butt_quest1.png" key="butt.quest.change">
      <!--퀘스트 전환 버튼-->
    </_butt>
  </quest_list>
  <_popup_account_regist>
    <img posx="0" posy="0" file="common_bg_popup.png" format="argb8888">
      <text posx="30" posy="92" font="font_text_b" text_col="white" font_size="35" font_style="stroke" align="left" width="112" text="80081" />
      <text posx="30" posy="160" font="font_text_b" text_col="white" font_size="35" font_style="stroke" align="left" width="112" text="80145"></text>
      <text posx="30" posy="218" font="font_text_b" text_col="white" font_size="35" font_style="stroke" align="left" width="112" text="80146" />
      <img posx="141" posy="78" file="option_input_box.png" key="img.box.email" format_surface="argb8888">
        <text posx="0" posy="11" width="284" font="font_text_b" align="center" font_size="43" font_style="stroke" key="text.email" />
      </img>
      <img posx="141" posy="146" file="option_input_box.png" key="img.box.password" format_surface="argb8888">
        <text posx="0" posy="11" width="284" font="font_text_b" align="center" font_size="43" font_style="stroke" key="text.password" />
      </img>
      <img posx="141" posy="214" file="option_input_box.png" key="img.box.password.verify" format_surface="argb8888">
        <text posx="0" posy="11" width="284" font="font_text_b" align="center" font_size="43" font_style="stroke" key="text.password.verify" />
      </img>
    </img>
    <butt posx="148" posy="274" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="30" key="butt.popup.register" font_style="stroke" text="80142"></butt>
    <butt posx="279" posy="274" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.close" font_style="stroke" text="80143"></butt>
  </_popup_account_regist>
  <popup_fail>
    <img posx="0" posy="0" file="guild_popup_bg.png">
      <img posx="54" posy="77" file="guild_popup_box.png">
        <text posx="7" posy="14" width="294" font="font_text_b" align="center" font_size="38" font_style="stroke" text="80147" key="text.regist.fail" />
      </img>
    </img>
    <butt posx="160" posy="183" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.close" font_style="stroke" text="80009"></butt>
    <!--잘못된 이멜임.-->
  </popup_fail>
  <!--텍스트 팝업-->
  <!--어떤창인지 모르겠다!-->
  <commontextpopup>
    <img posx="62" posy="9" file="common_bg_popup.png" key="img.common.popuptext">
      <!--타이틀-->
      <text posx="3" posy="17" font="font_text_b" text_col="white" font_size="43" font_style="stroke" align="center" key="text.title" />
      <!--설명 bg-->
      <img posx="144" posy="100" file="common_blank_b.png"></img>
      <!--메세지-->
      <text posx="147" posy="180" width="228" key="text.common.ask" align="center" font="font_text_m" font_size="30" font_style="stroke" />
      <!--왼쪽 버튼-->
      <butt posx="150" posy="270" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.ok" font_style="stroke" text="2"></butt>
      <!--오른쪽 버튼-->
      <butt posx="280" posy="270" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.cancel" font_style="stroke" text="3"></butt>
      <!--버튼 하나용-->
      <butt posx="215" posy="270" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.ok1" font_style="stroke" text="2" show="0"></butt>
    </img>
  </commontextpopup>
</layout>