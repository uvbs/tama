<?xml version="1.0" encoding="utf-8"?>
<!--무역상 씬-->
<layout font_text_m="normal.ttf" font_text_b="bold.ttf">
  <!--모든 씬 백판-->
  <common_bg>
    <wnd posx="0" posy="0" width="640" height="360" key="wnd.common_bg">
      <!--백판-->
      <img posx="0" posy="0" file="common_bg_bottom.png" />
      <!--백버튼-->
      <butt posx="28" posy="20" key="butt.back" img_up="common_butt_back.png" />
    </wnd>
  </common_bg>
  <!--무역상 백판-->
  <trader_bg>
    <wnd posx="64" posy="57" width="640" height="303" key="wnd.trader_bg">
      <!--내부 백판-->
      <img posx="0" posy="0" file="common_bg_inside.png"></img>
      <!--현수막-->
      <img posx="10" posy="10" file="trader_bg_banner.png" />
      <!--무역상 케릭터-->
      <wnd posx="0" posy="0" width="0" height="0" key="wnd.trader.char" show="0">
        <!--무역상 캐릭터-->
        <img posx="10" posy="46" file="trader_bg_char.png" />
        <!--무역상 캐릭터 대사-->
        <text posx="14" posy="50" font="font_text_b" font_size="16" font_style="stroke" align="hcenter" width="104" text="80062" key="text.armory.char" text_col="white"></text>
      </wnd>
      <!--무역상 바로 불러오기-->
      <wnd posx="0" posy="0" width="0" height="0" key="wnd.trader.recall">
        <!--남은 시간 text-->
        <text posx="-5" posy="60" font="font_text_s" font_size="30" font_style="stroke" align="hcenter" text_col="yellow" width="150" text="2281"></text>
        <text posx="-5" posy="80" font="font_text_s" font_size="30" font_style="stroke" align="hcenter" text_col="yellow" width="150" text="80131"></text>
        <text posx="-5" posy="110" font="font_text_s" font_size="30" font_style="stroke" align="hcenter" text_col="white" width="150" key="text.trader.recalltime"></text>
        <!--바로 불러오기 버튼-->
        <butt img_up="trader_butt_recall.png" posx="25" posy="200" text_col="white" font="font_text_b" font_size="35" key="butt.trader.recall" font_style="stroke">
          <!--"즉시 호출" 텍스트-->
          <text posx="center" posy="12" font="font_text_b" font_size="30" font_style="stroke" text_col="white" text="80130"></text>
          <!--호루라기 이미지-->
          <img posx="25" posy="28" file="trader_whistle.png">
            <text posx="20" posy="center" font="font_res_num" font_size="25" font_style="stroke" text_col="white" key="text.trader.whistle"></text>
          </img>
          <!--캐쉬 이미지-->
          <img posx="22" posy="53" file="trader_cash.png">
            <text posx="25" posy="center" font="font_res_num" font_size="25" font_style="stroke" text_col="white" key="text.trader.cash"></text>
          </img>
        </butt>
      </wnd>
      <!--아이템 리스트 백판-->
      <img posx="133" posy="13" file="trader_bg_list.png" />
      <!--자원 슬롯 백판 1 ~ 5-->
      <img posx="142" posy="23" file="trader_bg_itemslot.png" format="argb8888" />
      <img posx="142" posy="77" file="trader_bg_itemslot.png" format="argb8888" />
      <img posx="142" posy="131" file="trader_bg_itemslot.png" format="argb8888" />
      <img posx="142" posy="185" file="trader_bg_itemslot.png" format="argb8888" />
      <img posx="142" posy="239" file="trader_bg_itemslot.png" format="argb8888" />
      <!--자원 이미지 1 ~ 5-->
      <img posx="150" posy="30" file="icon/icon_res_wood.png" scale="0.8" key="img.trader.wood" />
      <img posx="150" posy="83" file="icon/icon_res_iron.png" scale="0.8" key="img.trader.iron" />
      <img posx="150" posy="138" file="icon/icon_res_jewel.png" scale="0.8" key="img.trader.jewel"></img>
      <img posx="150" posy="193" file="icon/icon_res_sulfur.png" scale="0.8" key="img.trader.sulfur" />
      <img posx="150" posy="245" file="icon/icon_res_mandrake.png" scale="0.8" key="img.trader.mandrake" />
      <!--슬라이드 백판 1 ~ 5-->
      <img posx="195" posy="23" file="trader_bg_slideritem.png" />
      <img posx="195" posy="77" file="trader_bg_slideritem.png" />
      <img posx="195" posy="131" file="trader_bg_slideritem.png" />
      <img posx="195" posy="185" file="trader_bg_slideritem.png" />
      <img posx="195" posy="239" file="trader_bg_slideritem.png" />
    </wnd>
  </trader_bg>
  <!--슬라이더-->
  <slidertab>
    <wnd posx="0" posy="57" width="0" height="0" key="wnd.trader">
      <!--자원 슬라이드 텍스트 1 ~ 5-->
      <text posx="258" posy="36" font="font_res_num" font_size="20" font_style="stroke" align="hcenter" width="136" key="text.trader.resource0" text_col="white"></text>
      <text posx="258" posy="90" font_size="20" font_style="stroke" align="hcenter" width="136" key="text.trader.resource1" text_col="white" font="font_res_num"></text>
      <text posx="258" posy="144" font_size="20" font_style="stroke" align="hcenter" width="136" key="text.trader.resource2" text_col="white" font="font_res_num"></text>
      <text posx="258" posy="198" font_size="20" font_style="stroke" align="hcenter" width="136" key="text.trader.resource3" text_col="white" font="font_res_num"></text>
      <text posx="258" posy="252" font_size="20" font_style="stroke" align="hcenter" width="136" key="text.trader.resource4" text_col="white" font="font_res_num"></text>
    </wnd>
  </slidertab>
  <!--자원시세-->
  <resourcetab>
    <wnd posx="410" posy="60" width="640" height="360" key="wnd.trader.resource">
      <!--양피지 백판-->
      <img posx="0" posy="0" file="trader_bg_magnifier.png" format="argb8888">
        <text posx="center" posy="7" font="font_text_b" font_size="30" font_style="stroke" text="80048" key="text.trader.resource" text_col="white"></text>
      </img>
      <!--"자원시세"-->
      <!--아이템 이미지-->
      <img posx="57" posy="40" key="img.trader.itemimg"></img>
      <!--자원 시세 텍스트 1 ~ 5-->
      <text posx="62" posy="55" font="font_text_b" font_size="20" font_style="stroke" align="hcenter" width="88" key="text.trader.rate0" text_col="white"></text>
      <text posx="62" posy="91" font="font_text_b" font_size="20" font_style="stroke" align="hcenter" width="88" key="text.trader.rate1" text_col="white"></text>
      <text posx="62" posy="127" font="font_text_b" font_size="20" font_style="stroke" align="hcenter" width="88" key="text.trader.rate2" text_col="white"></text>
      <text posx="62" posy="163" font="font_text_b" font_size="20" font_style="stroke" align="hcenter" width="88" key="text.trader.rate3" text_col="white"></text>
      <text posx="62" posy="199" font="font_text_b" font_size="20" font_style="stroke" align="hcenter" width="88" key="text.trader.rate4" text_col="white"></text>
      <!--자원 이미지 1 ~ 5-->
      <img posx="19" posy="47" file="world_top_wood.png" scale="1.2" />
      <img posx="21" posy="83" file="world_top_iron.png" scale="1.2" />
      <img posx="19" posy="119" file="world_top_jewel.png" scale="1.2" />
      <img posx="19" posy="154" file="world_top_sulfur.png" scale="1.2" />
      <img posx="23" posy="187" file="world_top_mandrake.png" />
      <!--총 변환 금액-->
      <text posx="39" posy="225" font_size="35" font_style="stroke" align="hcenter" width="88" key="text.trader.totalgold" text_col="white" font="font_res_num"></text>
      <!--금화 이미지-->
      <img posx="23" posy="223" file="world_top_gold.png" />
      <!--거래 버튼-->
      <butt img_up="common_butt_small.png" posx="7" posy="249" text_col="white" font="font_text_b" font_size="30" key="butt.trader.trade" font_style="stroke" text="80028" />
      <!--취소 버튼-->
      <butt img_up="common_butt_small.png" posx="82" posy="249" text_col="white" font="font_text_b" font_size="30" key="butt.trader.cancel" font_style="stroke" text="3" />
    </wnd>
  </resourcetab>
  <!--텍스트 팝업-->
  <_commontextpopup>
    <img posx="center" posy="center" file="popup_bg.png" key="img.common.popuptext" format="argb8888">
      <!--타이틀-->
      <img posx="center" posy="-15" img="popup_title_bar.png">
        <text posx="center" posy="4" font="font_text_b" text_col="white" font_size="43" font_style="stroke" align="hcenter" key="text.title" />
      </img>
      <!--설명 bg-->
      <img posx="center" posy="center" file="common_blank_b.png">
        <text posx="center" posy="center" key="text.common.ask" font="font_text_m" font_size="30" font_style="stroke" />
      </img>
      <!--메세지-->
      <!--왼쪽 버튼-->
      <_butt posx="center" posy="240" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.ok" font_style="stroke" text="2" />
      <!--오른쪽 버튼-->
      <_butt posx="280" posy="270" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.cancel" font_style="stroke" text="3" />
      <butt posx="right" posy="top" img_up="common_butt_x.png" key="butt.cancel"></butt>
    </img>
  </_commontextpopup>
  <!--아이템 슬롯 텍스트 팝업-->
  <_commonitemtextpopup>
    <img posx="center" posy="center" file="popup_bg.png" key="img.common.popupitemtext" format="argb8888">
      <!--타이틀-->
      <img posx="center" posy="-15" img="popup_title_bar.png">
        <text posx="center" posy="center" font="font_text_b" text_col="white" font_size="43" font_style="stroke" key="text.title" />
      </img>
      <!--설명 bg-->
      <img posx="center" posy="center" file="common_blank_b.png">
        <text posx="center" posy="80" key="text.common.ask" font="font_text_m" font_size="30" font_style="stroke" width="235" />
        <img posx="center" posy="15" file="common_bg_item.png">
          <img posx="center" posy="center" key="img.ask.itemimg"></img>
        </img>
      </img>
      <!--아이템 슬롯-->
      <!--아이템 이미지-->
      <!--메세지-->
      <!--왼쪽 버튼-->
      <butt posx="center" posy="240" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.ok" font_style="stroke" text="2"></butt>
      <!--오른쪽 버튼-->
      <_butt posx="280" posy="270" img_up="common_butt_mid.png" text_col="white" font="font_text_b" font_size="35" key="butt.cancel" font_style="stroke" text="3" />
      <butt posx="right" posy="top" img_up="common_butt_x.png" key="butt.cancel"></butt>
    </img>
  </_commonitemtextpopup>
</layout>