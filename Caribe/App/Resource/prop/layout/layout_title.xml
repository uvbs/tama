      </img>
      <_text posx="center" posy="349" text_col="white" font_size="15" font_style="stroke" text="2285">
        <!--gemtree-->
      </_text>
      <img file="logo.png" posx="center" posy="348" />
    </wnd>
    <butt posx="0" posy="0" width="scr_width" height="scr_height" key="butt.touch" debug="93" />
